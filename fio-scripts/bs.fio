########################################################################
##
##   Description:
##     A simple fio script to IOPS at increasing blocksize. Useful
##     for CPU utilization.
##
########################################################################

[global]
rw=randrw
random_generator=lfsr
direct=0
group_reporting=1
runtime=${RUNTIME}
filename=${FILENAME}
ioengine=${IOENGINE}
iodepth=${IODEPTH}
numjobs=${NUM_JOBS}
size=${SIZE}
rwmixread=${RW_MIX_READ}
thread

[bs_512]
bs=512

[bs_4k]
stonewall
bs=4k

[bs_8k]
stonewall
bs=8k

[bs_16k]
stonewall
bs=16k

[bs_64k]
stonewall
bs=64k

[bs_128k]
stonewall
bs=128k

[bs_1M]
stonewall
bs=1M
